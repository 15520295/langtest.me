{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "imgPath": "./assets/vocabulary/word/img",
          "soundPath": "./assets/vocabulary/word/sound",
          "underscore": "lodash"
        }
      }
    ]
  ]
}
