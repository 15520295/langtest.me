{
  "expo": {
    "name": "LangTestExpo",
    "description": "This project is really great.",
    "slug": "LangTestExpo",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "primaryColor": "#019AE8",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "assetExts": ["db"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rabbit.toeicfast"
    },
    "android":{
      "package": "com.rabbit.toeicfast"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000"
    }
  }
}