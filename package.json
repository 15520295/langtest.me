{
  "name": "empty-project-template",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "rimraf node_modules/.bin/expo* && expo start",
    "android": "rimraf node_modules/.bin/expo* && expo start --android",
    "ios": "rimraf node_modules/.bin/expo* && expo start --ios",
    "eject": "rimraf node_modules/.bin/expo* && expo eject",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc",
    "clean": "rimraf build",
    "build": "yarn run clean && yarn run tsc --",
    "watch": "yarn run build -- -w",
    "watchAndRunAndroid": "concurrently \"yarn run watch\" \"yarn run android\"",
    "buildRunAndroid": "yarn run build && yarn run watchAndRunAndroid ",
    "watchAndRunIOS": "concurrently \"yarn run watch\" \"yarn run ios\"",
    "buildRunIOS": "yarn run build && yarn run watchAndRunIOS ",
    "watchAndStart": "concurrently \"yarn run watch\" \"yarn run start\"",
    "buildAndStart": "yarn run build && yarn run watchAndStart "
  },
  "dependencies": {
    "@expo/vector-icons": "^8.0.0",
    "expo": "^30.0.1",
    "native-base": "^2.8.1",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-elements": "^0.19.1",
    "react-native-flip-card": "^3.5.2",
    "react-native-gesture-view": "^0.2.1",
    "react-native-pose": "^0.9.0",
    "react-native-slider": "^0.11.0",
    "react-native-super-grid": "^2.4.3",
    "react-native-typography": "^1.4.0",
    "react-navigation": "^2.18.2",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@types/expo": "^30.0.0",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-native": "^0.57.7",
    "@types/react-navigation": "^2.13.0",
    "@types/react-test-renderer": "^16.0.3",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.0.1",
    "eslint": "^5.7.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
